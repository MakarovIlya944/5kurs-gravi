      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Main: learn",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "learn",
        "cnn_flat",
        "big_2_1000"
      ]
    },
    {
      "name": "Main: solidity",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "solid",
        "thin_small",
        "avg"
      ]
    },
    {
      "name": "Main: predict",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "predict",
        "cnn",
        "-n","0",
        "-m","0",
        "-s","true"
      ]
    },
    {
      "name": "Main: inspect",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "inspect",
        "net",
        "--dataset-config",
        "clean",
        "--dataset",
        "points",
        // "-n","10",
        "--model","12-15-22-33_clean_1-points","--model-config","clean_1"
