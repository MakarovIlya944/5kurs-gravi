  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Текущий файл",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Main: learn",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "learn",
        "cnn_flat",
        "cnn_2"
      ]
    },
    {
      "name": "Main: solidity",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "solid",
        "thin_small",
        "avg"
      ]
    },
    {
      "name": "Main: predict",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "predict",
        "cnn",
        "-n","0",
        "-m","0",
        "-s","true"
      ]
    },
    {
      "name": "Main: inspect",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "inspect",
        "response",
        "--dataset-config",
        "cnn_2",
        "--dataset",
        "cnn_2",
        // "-n","10",
        "--model","01-10-21-22_cnn_flat-cnn_2","--model-config","cnn_flat"
      ]
    }
  ]
}