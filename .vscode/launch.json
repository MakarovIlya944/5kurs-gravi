      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "inspect",
        "stat",
        "--dataset-config",
        "fill",
        "--dataset",
        "even"
        // "-n","10",
