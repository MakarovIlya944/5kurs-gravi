    },
    {
      "name": "Main: data",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "data",
        "--fill",
        "--name",
        "even",
        "fill",
        "20"
      ]
    },
    {
      "name": "Main: learn",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "learn",
        "cnn_flat_6",
        "even"
      ]
    },
    {
      "name": "Main: solidity",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "solid",
        "thin_small",
        "avg"
      ]
    },
    {
      "name": "Main: show",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
